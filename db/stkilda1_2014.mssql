(153, 8, 'opening-closing-night-tix', 'OPENING & CLOSING NIGHT TIX', '<p><span class="whiteheader"><span class="heading">OPENING &amp; <span class="whiteheader">CLOSING NIGHT TICKETS </span></span></span></p>\n<p><span class="whiteheader">\n</span></p>\n<hr />\n<p>&nbsp;</p>\n<p>&nbsp;</p>\n<table style="padding-top: 10px;">\n    <tbody>\n        <tr>\n            <td class="heading"><br />\n            </td>\n            <td style="padding-top: 15px;" rowspan="5" valign="top" width="250"><br />\n            </td>\n            <td style="padding-top: 15px;" rowspan="5" valign="top" width="250"><br />\n            <img alt="" src="/skf2015/uploads/Opening Night_R.jpg" width="250" border="0" height="201" /></td>\n        </tr>\n        <tr>\n            <td style="width: 980px; height: 215px;" class="normaltext1">\n            <p><font face="Arial"><font class="subheading1" face="Arial">OPENING NIGHT</font></font></p>\n            <p>&nbsp;</p>\n            <p><font color="#ffffff" face="Arial"><span class="normaltext1">PALAIS THEATRE<br />\n            TUESDAY 22 MAY 7.30PM<br />\n            </span>\n            </font></p>\n            <p><font color="#ffffff" face="Arial"><span class="normaltext1">&nbsp;</span></font></p>\n            <font color="#ffffff" face="Arial">\n            <span class="normaltext1">Walk the red carpet at St Kilda&#195;&#162;&#226;‚&#172;&#226;„&#162;s magnificent Palais Theatre as we celebrate the triumphs of our local film industry. Opening Night&nbsp;&nbsp;features a selection of what&#195;&#162;&#226;‚&#172;&#226;„&#162;s to come for the 2012 St Kilda Film Festival.<br />\n            </span>\n            <p><span class="normaltext1">&nbsp;</span></p>\n            <span class="normaltext1">$30 Full / $25 Concession</span>*<br />\n            </font>\n            <p>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n        <tr>\n            <td>\n            <p><span class="normaltext1"><em></em></span>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n    </tbody>\n</table>\n<p>&nbsp;\n</p>\n<hr />\n<p>&nbsp;</p>\n<p>\n<table style="padding-top: 10px;">\n    <tbody>\n        <tr>\n            <td class="heading"><br />\n            </td>\n            <td style="padding-top: 15px;" rowspan="5" valign="top" width="250"><br />\n            </td>\n            <td style="padding-top: 15px;" rowspan="5" valign="top" width="250"><br />\n            <img alt="" src="/skf2015/uploads/Closing Night_R.jpg" width="250" border="0" height="201" /></td>\n        </tr>\n        <tr>\n            <td style="width: 980px; height: 215px;" class="normaltext1">\n            <p><font face="Arial"><font class="subheading1" face="Arial">CLOSING NIGHT AWARDS</font></font></p>\n            <p>&nbsp;</p>\n            <p><font class="normaltext1" color="#ffffff" face="Arial">ST KILDA TOWN HALL<br />\n            SUNDAY 27 MAY 8.30PM<br />\n            </font></p>\n            <p><font class="normaltext1" color="#ffffff" face="Arial">&nbsp;</font></p>\n            <font class="normaltext1" color="#ffffff" face="Arial">\n            Join the celebrations as we wrap up the St Kilda Film Festival for yet another year.<br />\n            <p>&nbsp;</p>\n            Share in the excitement as the best of this year&#195;&#162;&#226;‚&#172;&#226;„&#162;s filmmakers are awarded over $40,000 of cash and in-kind prizes.<br />\n            <p>&nbsp;</p>\n            $25 Full / $20 Concession*</font></td>\n            <td>&nbsp;</td>\n        </tr>\n        <tr>\n            <td>\n            <p><span class="normaltext1"><em></em></span>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n    </tbody>\n</table>\n</p>\n<p><span class="subheading2">\n</span></p>\n<hr />\n<table style="padding-top: 10px;">\n    <tbody>\n        <tr>\n            <td class="heading"><br />\n            </td>\n            <td style="padding-top: 15px;" rowspan="5" valign="top" width="250"><br />\n            </td>\n            <td style="padding-top: 15px;" rowspan="5" valign="top" width="250"><br />\n            </td>\n        </tr>\n        <tr>\n            <td style="width: 980px; height: 215px;" class="normaltext1">\n            <p><font face="Arial"><font class="subheading1" face="Arial">HOW TO BOOK OPENING &amp; CLOSING NIGHT TICKETS</font></font></p>\n            <p>&nbsp;</p>\n            <p><font color="#ffffff" face="Arial"><font color="#ffffff" face="Arial"><span class="normaltext1">Phone 03 9209 6699 or visit the St Kilda Film Festival Office at Level 1, 232 Carlisle Street, St Kilda East.<br />\n            </span>\n            </font></font></p>\n            <p><font color="#ffffff" face="Arial"><font color="#ffffff" face="Arial"><span class="normaltext1">&nbsp;</span></font></font></p>\n            <font color="#ffffff" face="Arial"><font color="#ffffff" face="Arial">\n            <span class="normaltext1">Cash, Eftpos, Credit Card facilities available.<br />\n            (Office hours: 9am - 5pm weekdays).<br />\n            </span>\n            <p><span class="normaltext1">&nbsp;</span></p>\n            <span class="normaltext1">Opening and Closing Night tickets are sold on a no refund, exchange or cancellation basis.<br />\n            </span>\n            </font></font>\n            <p><font color="#ffffff" face="Arial"><font color="#ffffff" face="Arial">\n            </font></font></p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n        <tr>\n            <td>\n            <p><span class="normaltext1"><em></em></span>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n    </tbody>\n</table>\n<p><span class="subheading2">Opening and Closing Night tickets are strictly limited and will sell-out! </span></p>\n<p>&nbsp;</p>\n<p>&nbsp;</p>', '2012-04-24 12:05:08', '2014-03-31 01:32:26'),
(159, 9, 'accommodation', 'ACCOMMODATION', '<p><span class="heading">ACCOMMODATION </span>\n<hr />\n</p>\n<p>&nbsp;</p>\n</font>\n<p>\n<table style="padding-top: 10px">\n    <tbody>\n        <tr>\n            <td class="heading"></td>\n            <td style="padding-top: 15px" valign="top" rowspan="5" width="250"><span class="normaltext1">&nbsp;</span></td>\n            <td style="padding-top: 15px" valign="top" rowspan="5" width="250"><br />\n            <span class="normaltext1"><img border="0" alt="" src="/skf2015/uploads/Accommodation_R.jpg" width="250" height="201" /></span></td>\n        </tr>\n        <tr>\n            <td style="width: 980px; height: 215px" class="normaltext1">\n            <p><span class="normaltext1"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><span class="subheading1"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><span class="subheading1"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><span class="subheading2"><font color="#702c91" face="Arial"><font class="subheading1" color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font color="#702c91" face="Arial"><font class="subheading1" color="#702c91" face="Arial">COSMOPOLITAN HOTEL</font></font> </font></font></font></font><br />\n            <br />\n            </font></font></span></font></font></font></font></span></font></font></span><span style="font-size: 8pt"><span class="normaltext1"><span style="font-size: 10pt"><span class="normaltext1"><span class="normaltext1"><span style="font-size: 10pt"><span class="normaltext1"><span class="normaltext1"><span class="normaltext1">Located in the heart of St Kilda and on&nbsp;the doorstep of Luna Park, The Esplanade </span></span></span></span>and St Kilda Beach, the Cosmopolitan Hotel stands out with its comfortable interiors, fantastic service and incomparable value for money. This landmark icon has been im<span style="font-size: 10pt">maculat</span>ely redesigned with the latest luxury amenities and variety of accommodation types<span class="normaltext1">, from single rooms to two bedroom apartments. The Cosmopolitan Hotel is perfectly positioned to insure an unforgettable and truly Ã¢â‚¬ËœMelbourneÃ¢â‚¬â„¢ experience</span></span></span><span class="normaltext1">.</span><br />\n            &nbsp;<br />\n            <span class="normaltext1">2-8 Carlisle Street, St Kilda VIC 3182<br />\n            (03) 9534 0781 <br />\n            <a href="http://www.8hotels.com/melbourne-hotels/cosmopolitan-hotel/" target="_blank"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1">cosmopolitanhotel.com.au</span></font></span></a></span></span></span></span></span></font></span></font></span></span></p>\n            <p><span class="normaltext1"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><font color="#702c91" face="Arial"></font></span></font></span></span>&nbsp;</p>\n            <p><span class="normaltext1"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><font color="#702c91" face="Arial"><span class="normaltext1"><img style="width: 130px; height: 85px" border="0" alt="" src="/skf2015/uploads/Cosmo_R.jpg" width="130" height="85" /></span></font></span></font></span></span></p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n        <tr>\n            <td>\n            <p><span class="normaltext1"><em></em></span>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n    </tbody>\n</table>\n</p>\n', '2012-05-01 10:13:07', '2012-05-15 17:47:26'),
(158, 1, 'win-stuff', 'WIN STUFF', '<p><span class="heading">THE ULTIMATE ST KILDA FILM FESTIVAL PRIZE PACK\n<p>\n<p>&nbsp;</p>\n<p>\n<hr />\n</span>\n<p>&nbsp;</p>\n<p>&nbsp;</p>\n<p>&nbsp;<span class="subheading1">THIS COMPETITION HAS NOW CLOSED. WE HAVE A VERY HAPPY WINNER!</span>\n<div align="center">&nbsp;</div>\n<p>&nbsp;</p>\n<p align="center">&nbsp;</p>\n<div align="center"><img style="width: 60px; height: 48px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="60" height="48" /></div>\n<br />\n<br />\n<p align="center"><font face="Arial"><font face="Arial"><span class="normaltext1"><font face="Arial"><span class="normaltext1"><font style="color: #702c91" class="normaltext1" face="Arial"><span class="normaltext1"><font style="color: #702c91" class="normaltext1" face="Arial"><span class="normaltext1"><font style="color: #702c91" face="Arial"><font style="color: #702c91" face="Arial"><span class="normaltext1"><strong>2X VIP TICKETS TO ST KILDA FILM FESTIVAL OPENING NIGHT &amp; AFTER PARTY</strong><br />\nPalais Theatre</span><br />\n<span class="normaltext1"><font style="color: #702c91" face="Arial"><font style="color: #702c91" face="Arial"><span class="normaltext1">Tuesday 22 May 7.30pm</span></font></font><font style="color: #702c91" face="Arial"> </font></span></font></font></span></font></span></font><br />\n</p>\n<font style="color: #702c91" face="Arial"><span class="normaltext1"><font style="color: #702c91" face="Arial"><span class="normaltext1"><strong>\n<p align="center"><img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" /><br />\nOVERNIGHT </strong></span><span class="normaltext1"><strong>ACCOMMODATION FOR 2 AT COSMOPOLITAN HOTEL on Opening Night, Tuesday&nbsp;22nd May&nbsp;</strong> <br />\nAccommodation is only valid for Tuesday 22nd May 2012.<br />\nThe terms and conditions of <span class="normaltext1">any third party will apply to the prize where applicable. <br />\nEach prize must be taken as stated and no compensation will be payable if a winner is unable to use the prize as stated.</span></span></font></span></font></p>\n<p align="center"><img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" /><br />\n&nbsp;</p>\n<p align="center"><span class="normaltext1"><strong>RENAULT LATITUDE VEHICLE TRANSFERS</strong><br />\nYou will be escorted to and from the opening night of the St Kilda Film Festival in luxury, with thanks to our friends at Renault</span></p>\n<p align="center"><br />\n<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" />&nbsp;</p>\n<p align="center"><font style="color: #702c91" face="Arial"><span class="normaltext1"><strong>2X ST KILDA FILM FESTIVAL GOLDEN TICKETS</strong><br />\nYour passes to everything St Kilda Film Festival. Includes all Top 100 competition sessions, international programs, youth and special screenings, plus the mighty SoundKILDA Ã¢â‚¬â€œ Australian Music Video competition</span></font></p>\n<p align="center"><br />\n&nbsp;<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" /></p>\n<p align="center"><font style="color: #702c91" class="normaltext1" face="Arial"><span class="normaltext1"><strong>2X ST KILDA FILM FESTIVAL CLOSING NIGHT AWARDS TICKETS<br />\n</strong>St Kilda Town Hall, <br />\nSunday 27 May 8.30pm </span></font></p>\n<p align="center"><br />\n<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" />&nbsp;</p>\n<p align="center"><font style="color: #702c91" face="Arial"><span class="normaltext1"><font style="color: #702c91" face="Arial"><span class="normaltext1"><strong>BEVERAGE SAMPLE PACK<br />\n</strong>Mixed sample pack of Rekorderlig Cider, Xanadu Next of Kin Wines and Genovese Coffee. 1 x case of James Boags Premium Lager and 1 bottle of Budaberg <span class="normaltext1">White Rum</span></span></font></span>.</font></p>\n<font style="color: #702c91" face="Arial">\n<p align="center"><br />\n&nbsp;<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" /></p>\n<p align="center"><span class="normaltext1"><strong>2 X DOUBLE </strong></span><span class="normaltext1"><strong>CINEMA NOVA PASSES</strong> <br />\n2 x double passes to any regular session valid to 01 Dec 2012*<br />\n* Subject to availability</span>.</p>\n<p align="center"><br />\n<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" />&nbsp;</p>\n<p align="center"><span class="normaltext1"><strong>ASTOR THEATRE GOLD PASS</strong> <br />\nIncludes two free tickets to one session per calendar week across the current Astor Theatre calendar -May - August 2012</span></p>\n<p align="center"><br />\n&nbsp;<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" /></p>\n<p align="center"><span class="normaltext1"><strong>ST KILDA FILM FESTIVAL T-SHIRT<br />\n</strong>1 x Official St Kilda Film Festival T-Shirt</span></p>\n<p align="center"><br />\n&nbsp;<img style="width: 30px; height: 25px" border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Star_Rs.jpg" width="30" height="25" /></p>\n<p class="normaltext1" align="center"><strong>CRUMPLER BACKPACK<br />\n</strong>1 x stylish backpack with thanks to Crumpler</p>\n<p class="normaltext1">&nbsp;</p>\n<p class="normaltext1">THE ULTIMATE PRIZE PACK WILL BE DRAWN 5PM FRIDAY 18 MAY. THE LUCKY WINNER WILL BE NOTIFIED BY PHONE.</p>\n<p><a href="http://www.stkildafilmfestival.com.au/2012-s2/page-173/ultimate-stkildafilmfestival-prize-competition"><img border="0" alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Enter-Here-Button2.jpg" width="200" height="93" /></a>&nbsp;<span class="subheading1">THIS COMPETITION HAS NOW CLOSED, WE HAVE A VERY HAPPY WINNER</span></p>\n<p><a href="http://www.stkildafilmfestival.com.au/2012-s2/page-116/Terms-and-Conditions-Privacy-Policy">Terms &amp; Conditions/ Privacy Policy</a></p>\n</font></span></font></span></font></font>\n', '2012-04-26 11:48:55', '2012-05-21 10:21:50'),
(198, 8, 'venues', 'Venues', '<strong><span class="heading-content2"></span></strong><span class="heading-content1"><strong><span class=""><span class=""><span class="">VENUES&nbsp; </span></span></span></strong></span><br />\n<br />\n<p><span class="heading-content3">PALAIS THEATRE<br />\n<span class="content-text">Lower Esplanade, St Kilda<br />\n</span></span></p>\n<span class="heading-content3">\n<br />\n<span class=""> THE ASTOR THEATRE</span></span><br />\n<span class="content-text"><span class="">Cnr Chapel St &amp; Dandenong Rd, St Kilda</span></span><br />\n<br />\n<span class="heading-content3"><span class="">FORUM HALL</span></span><br />\n<span class="content-text"><span class="">All Saints Church, Over the road from The Astor Theatre,<br />\nCnr Chapel St &amp; Dandenong Rd, St Kilda</span></span><br />\n<br />\n<span class="heading-content3"><span class="">ST KILDA TOWN HALL</span></span><br />\n<span class="content-text"><span class="">Cnr Carlisle St &amp; Brighton Rd, St Kilda</span></span><br />\n<br />\n<br />\n<span class="content-text">For train, tram and bus information call Public Transport Victoria on 1800 800 007 6am – midnight daily or visit <a target="_blank" href="http://PTV.VIC.GOV.AU">ptv.vic.gov.au</a></span>', '2013-04-15 15:35:20', '2014-03-30 23:59:53'),
(197, 1, 'airasia-festival-lounge', 'AirAsia Festival Lounge', '<img alt="" src="/skf2015/uploads/AirAsiaLounge-FullWidth-3.jpg" width="710" height="300" /><br />\n<br />\n<br />\n<div align="left"><span class="subheading2">THE</span>&nbsp; <img alt="" src="/skf2015/uploads/AirAsiaLogo.png" width="95" align="textTop" height="95" /><span class="subheading2"> </span><span class="subheading2">LOUNGE</span><br />\n</div>\n<br />\n<span class="normaltext1"><br />\nProudly brought to you by AirAsia, the official Festival lounge is upstairs at The Astor Theatre. It’s the perfect place to relax, talk film with friends, network and soak up the atmosphere. <br />\n<br />\nHave a coffee, buy a choc top or while away the night with a glass of wine or beer.<br />\n<br />\nFor ten days at the end of May, it’s the best club in town. </span>', '2013-04-15 15:25:13', '2013-05-07 18:52:43'),
(199, 11, 'accommodation', 'Accommodation', '<span class="heading"><img src="http://www.stkildafilmfestival.com.au/skf2015/uploads/CosmopolitanHotel.jpg" alt="" width="850" height="345" /></span>\n<div style="height: 20px">&nbsp;</div>\n<div class="calendar-purple-line"> </div>\n<table class="table news-table">\n    <tbody>\n        <tr>\n            <td>\n            <h1 class="heading-content1">COSMOPOLITAN\n            HOTEL </h1>\n            <h2 class="heading-content2">St\n            Kilda Film Festival Accommodation Partner </h2>\n            <h3 class="heading-content3">2-8\n            Carlisle St, St Kilda <br />\n            For reservations call 03 9534 0781 or go to <a target="_blank" href="http://www.8hotels.com">www.8hotels.com</a> </h3>\n            <p class="content-text"> Located\n            in the heart of St Kilda on the doorstep of Luna Park, The Esplanade, St Kilda\n            Arts Markets, St Kilda Beach and Melbourne''s finest restaurants and shops, the\n            Cosmopolitan Hotel stands out with its boutique interiors, breathtaking\n            hospitality and incomparable value for money. This landmark icon immaculately\n            redesigned with the latest luxury amenities and variety of accommodations to\n            suit all needs, will assure an unforgettable and truly authentic Melbourne\n            experience. Part of the 8Hotels Collection, this is a hotel experience not to\n            be missed.  <br />\n            <br />\n            </p>\n            <p style=" line-height:normal"><img class="img-no-bootstrap" src="data:image/png;base64," alt="" /><span style="font-size:12.0pt; font-family:&quot;Times New Roman&quot;,&quot;serif&quot;;Times New Roman&quot;;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><img class="img-no-bootstrap" src="data:image/png;base64," alt="" /></p>\n            <p style=" line-height:normal">&nbsp;</p>\n            <p class="content-text"><br />\n            <br />\n            <br />\n            </p>\n            </td>\n        </tr>\n    </tbody>\n</table>', '2013-04-15 15:39:28', '2014-04-01 05:40:25'),
(194, 15, '2013--the-big-picture', '2013 -The Big Picture', '<p class="normaltext1"><span class="heading"><br />\nTHE BIG PICTURE</span><span><span class="subheading1"><br />\n</span></span><br />\nPresented by Deluxe Entertainment Services Group<br />\n<br />\n<a target="_blank" href="http://www.bydeluxe.com"><img alt="" src="/skf2015/uploads/DeluxeRedCircle.png" width="95" height="95" /></a><br />\n</p>\n<p><span class="normaltext2"><span class=""><span class="subheading2">\n<br />\n</span></span></span><span class="subheading1">THE ASTOR THEATRE<br />\n</span><span class="subheading2"><span class=""><span class="">SATURDAY 25 MAY<br />\n11AM-4PM</span></span></span><br />\n</p>\n<p><span class="normaltext2"><span class=""><span class=""><br />\nThe film industry takes over The Astor for one big day of professional development.<br />\n<br />\nAs part of the St Kilda Film Festival’s Filmmaker Development program, The Big Picture covers all you need to know about making a short film, from start to finish. With forum topics including scriptwriting, funding, legal, production and distribution, you will end the day with a wealth of knowledge from some of the industry’s most experienced and awarded filmmakers and film professionals. Moderated by Shaun Miller.<br />\n<br />\nAnd if that isn’t enough, you can continue the conversations face-to-face with stallholders at our <strong>Industry Open Day</strong>, with representatives from pre- and post-production houses, education bodies and filmmaker guilds hosting information booths upstairs at The Astor.</span></span></span><br />\n<span class="normaltext1"><br />\n</span><span class="normaltext1">Following The Big Picture, join us for an ongoing series of talks happening <a href="http://localhost/skf2015/page-195/2013---throughout-the-week">throughout the Festival week</a>. </span><span class="normaltext1">All Filmmaker Development events are FREE. Entry is on a first come, first served basis.</span><br />\n</p>\n<hr />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <span class="subheading1"><span class="">THE WRITE STUFF</span></span><br />\n            <span class="subheading2">11.10AM</span><br />\n            <br />\n            The job of a screenwriter is to tell a story and to tell it well. But how do you do that? What are the essential ingredients and how do you know if writing is the right path for you? Join writers Peter Moon, Mac Gudgeon (pictured), Kris Mrksa and Natasha Pincus as they explore the ups and downs of the writing process and the collaborations that have helped make their projects successful.<font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"><span class="normaltext2"><br />\n            <br />\n            Presented by the Australian Writers Guild&nbsp; <a target="_blank" href="http://www.awg.com.au"><img alt="" src="/skf2015/uploads/awg-5years-logo-HERO-cmyk.jpg" width="75" align="top" height="44" /></a></span></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-TheWriteStuff-MacGudgeon.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">SHOW ME THE MONEY</font><font class="subheading2" face="Lato"><br />\n            12.05PM</font><br />\n            <br />\n            Money doesn’t grow on trees, and neither does funding for short film (unfortunately!). Finance for short films can be sourced from private investors and donors, government funding, crowd funding, deferrals, “sweat equity” or in-kind favours. This session will look at the “how, why, where and when” each alternative works best. You will then be in a better informed position to pay for the realisation of your short film. Panellists: Film Victoria''s Acting Head of Screen Industry Programs Sue Edwards, Screen Australia''s Development Executive Katherine Slattery, producers Jannine Barnes and Michael Ciccone, and Anna Whitelaw, crowd funding for film expert. Photo by James Thomas.<br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-ShowMetheMoney-Ciccone.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">LAW, CAMERA, ACTION!</font><font class="subheading2" face="Lato"><br />\n            1PM<br />\n            <br />\n            </font> </p>\n            <p> This session will explore legal issues relating to financing, production and distribution of films and television which every emerging filmmaker should be aware of. The session will be interactive with the audience where you get to cross-examine Jenny Lalor and Shaun Miller (pictured) while they provide the evidence to hopefully let you rest your case. Come along and witness this legal drama!<br />\n            <br />\n            Presented by Shaun Miller Lawyers <a target="_blank" href="http://www.shaunmillerlawyers.com.au"><img alt="" src="/skf2015/uploads/ShaunMiller-Logo.jpg" width="186" align="top" height="27" /></a><br />\n            <font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-LawCameraAction-ShaunMiller.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">PRODUCTION: THE CHANGING TECHNOLOGICAL LANDSCAPE</font><font class="subheading2" face="Lato"><br />\n            1.50PM</font> </p>\n            <p> <br />\n            The creators of award winning series <em>The Slap</em> are stopping by to talk production – pre-production, the shoot and post processes. Digital production is changing the way we make films – not just in the way we store files and the choice in digital cameras, but also the way we shoot and the way we grade and finish. Come along to this forum to learn from the people that get it right. Panellists: Matt Saville, Michael McMahon, Ross Mitchell, Tim Schumann and Melinda Doring.</p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-Production-TheSlap.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">DISTRIBUTING FILMS: THE MORE PLATFORMS THE MERRIER</span><br />\n            <span class="subheading2">3PM<br />\n            </span><br />\n            </p>\n            <p class="normaltext1"> Get the lowdown on how best to distribute and market your short film both locally and internationally. This session will explore the array of media platforms in which short films can be distributed and marketed including film festivals, television and new media platforms such as mobile phones and other portable devices. Commercial tips will be offered and legal pitfalls to be wary of will be highlighted. Panellists: Grant Scicluna (pictured<em>, The Wilding</em>), Simon Britton (MediaWave TV), Katharine Thornton (Distribution Manager Sharmill Films), Claudette Godfrey (Short Film Programmer &amp; Film Festival Coordinator at SXSW) and Kristian Connelly (Cinema Nova General Manager).</p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-Distribution-GrantScicluna.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">LIVE BROADCAST: FILMBUFF''S FORECAST</span><br />\n            <span class="subheading2">12PM-2PM<br />\n            </span><br />\n            </p>\n            <p class="normaltext1"> Don’t miss Festival Director <a target="_self" href="http://localhost/skf2015/page-179/festival-director">Paul Harris</a> in Filmbuff’s Forecast, broadcast live from the Astor Foyer Theatre from 12pm until 2pm.</p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/RRR-Filmbuffs.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />', '2013-04-12 14:30:54', '2013-05-14 11:13:35');
INSERT INTO `pages` (`id`, `category_id`, `name`, `title`, `content`, `created`, `modified`) VALUES
(195, 15, '2013---throughout-the-week', '2013 - Throughout the Week', '<p><span class="heading"><br />\nTHROUGHOUT THE WEEK<br />\n</span><br />\n</p>\n<p class="normaltext1"><span class="normaltext1">Enjoy a day of forums on Saturday 25 May with <a href="http://localhost/skf2015/page-194/2013--the-big-picture">The Big Picture</a> then join us for an ongoing series of talks happening throughout the Festival week.<br />\n<br />\nAll of the below events will be held at the Forum Hall in All Saints Church, opposite The Astor on the corner of Chapel St &amp; Dandenong Rd, St Kilda</span><br />\n<br />\nAll Filmmaker Development events are FREE. Entry is on a first come, first served basis. <br />\n</p>\n<hr />\n<a name="perfectmatch"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p><span class="subheading1">FINDING YOUR PERFECT MATCH: RELATIONSHIP BETWEEN THE DIRECTOR AND CINEMATOGRAPHER</span><br />\n            <font class="subheading2" face="Lato">MONDAY 27 MAY, 6.30PM</font><br />\n            <br />\n            The relationship between the Director and the Cinematographer has often been likened to a creative ''marriage''. Like any successful relationship it takes commitment, respect, trust and understanding. This forum will explore some of the traits essential for successful creative collaborations, as well as techniques to get the best out of each other. Featuring collaborators Natasha Pincus and Warwick Field. Moderated by Victoria Goodyear.<br />\n            <br />\n            </p>\n            <p>\n            Presented by Australian Directors Guild and Australian Cinematographers Society<br />\n            <br />\n            </p>\n            <p><a target="_blank" href="http://www.adg.org.au/"><img alt="" src="/skf2015/uploads/ADG-logo.png" width="60" height="52" /></a>&nbsp;&nbsp;&nbsp; <a target="_blank" href="http://www.cinematographer.org.au"><img alt="" src="/skf2015/uploads/acs-logo.png" width="60" height="41" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-CineDir.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="spaa"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <span class="subheading1">SPAA FRINGE WORKSHOP - PITCHING TO A TV CHANNEL</span><br />\n            <font class="subheading2" face="Lato">MONDAY 27 MAY, 8PM</font><br />\n            <br />\n            The opportunity to pitch your idea direct to a TV channel is rare. Now here’s your chance! <br />\n            <br />\n            SPAA has teamed up with STUDIO, FOXTEL’s arts and entertainment channel to offer early-career producers the chance to get a project commissioned for the channel through SPAA''s new Ones to Watch mentor program and the STUDIO Kickstart initiative. Finalists can pitch their project and go into the running to receive a production budget of $300K. This workshop provides insight into applying for SPAA''s Ones to Watch program and STUDIOs exciting new production initiative.<br />\n            <br />\n            Presented by SPAA<br />\n            <a target="_blank" href="http://www.spaa.org.au"><img alt="" src="/skf2015/uploads/SPAA_logo.png" width="125" height="62" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-SPAA.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="thisisthespot"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">THIS IS THE SPOT</font><br />\n            <font class="subheading2" face="Lato">TUESDAY 28 MAY, 6.30PM</font><br />\n            <br />\n            What makes a good Location Manager? Finding a creative location that works well for the Director and Production Designer is one thing - you also need to navigate the red tape, keep within budget and schedule. The role of a Location Manager is vital and knowing the tricks of the trade can make or break a shoot. Get advice from the experts who''ve successfully sourced and managed locations for <em>The Slap, Rush, Tangle, Mr and Mrs Murder, Canal Road, Miss Fisher''s Murder Mysteries, Ghost Rider</em> and <em>The Pacific</em>. Moderated by Neil McCart with panellists Pia Emery, Mel Dunstone, Alistair Riley and Drew Rhodes.<br />\n            <br />\n            <br />\n            <br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/Top%20100_R.jpg" width="230" border="0" height="150" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="ifiknewthen"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top">\n            <p> <font class="subheading1">IF I KNEW THEN WHAT I KNOW NOW</font><br />\n            <font class="subheading2" face="Lato">TUESDAY 28 MAY, 8PM</font> </p>\n            <p> <br />\n            Can you learn from other people''s hits and misses? Is it true that what doesn''t kill you makes you stronger? This lively panel may not exactly answer those questions, but it will have some fun exploring the journeys of some of the Festival''s favourite filmmakers along the way. Panellists: Clayton Jacobson, Emma Freeman, Julie Eckersley and Trevor Blainey. Moderated by Adam Elliot (pictured).<br />\n            <br />\n            <br />\n            <br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/FD-AdamElliot.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="focusonediting"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">FOCUS ON EDITING</span><br />\n            <span class="subheading2">WEDNESDAY 29 MAY, 6.30PM <br />\n            <br />\n            </span></p>\n            <p> <span class="normaltext1">Australia’s peak representational body for screen editing brings together some of the country’s finest editing talent to share their own techniques, knowledge and experiences. The session will use examples of each panellist’s work to dissect and analyse the post-production process. Open to directors, producers, editors and those captivated by the intricacies of this craft. <br />\n            <br />\n            Presented by Australian Screen Editors Guild<br />\n            <br />\n            <a target="_blank" href="http://www.screeneditors.com"><img alt="" src="/skf2015/uploads/ASE_logo.png" width="150" align="textTop" height="42" /></a></span></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-Editing.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="wannabe"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1"><span class="">SO, YOU WANNA BE A FILMMAKER?</span></span><br />\n            <span class="subheading2">WEDNESDAY 29 MAY, 8PM <br />\n            <br />\n            </span></p>\n            <p> <span class="normaltext1">An insight into the realities of working in the film industry, the highs and lows and the real opportunities that exist for emerging and aspiring filmmakers. Moderated by Caroline Waters, a panel of working practitioners including Jonathan auf der Heide, Darrell Martin, Gretchen Thornburn, Julie Belle and Daniel Schultheis will share their experiences and advice in this practical session.<br />\n            <br />\n            Presented by Open Channel </span><br />\n            <br />\n            <br />\n            <a target="_blank" href="http://www.openchannel.org.au"><img alt="" src="/skf2015/uploads/OpenChannelLogo.png" width="125" height="42" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-wannabeafilm.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="composing"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1"><span class="">THE ABCS OF COMPOSING FOR SCREEN</span></span><br />\n            <span class="subheading2">THURSDAY 30 MAY</span><span class="subheading2">, 6.30PM</span><br />\n            <span class="normaltext1"><br />\n            A special forum on composing original underscore for film and TV, with the simple aim of equipping filmmakers and composers with knowledge, language and insights to obtain the best results for their films. Some of Australia’s leading composers will join forces to share how music relates to the story and enhances the screen experience. Panellists include Cezary Skubiszewski (<em>The Sapphires, Red Dog, Bran Nue Dae</em>), Burkhard Dallwitz (pictured, <em>Underbelly</em>), Thomas Rouch (<em>Reverse Runner</em>) and Petra Salsjo (<em>Dave’s Dead</em>).<br />\n            <br />\n            Presented by APRA</span><br />\n            <br />\n            <a target="_blank" href="http://www.apra-amcos.com.au"><img alt="" src="/skf2015/uploads/APRA-REV.jpg" width="87" height="91" /></a><br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-Composing.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<a name="reclaiming"></a>\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p> <span class="subheading1">RECLAIMING IDENTITY: <br />\n            </span><span class="subheading1"><span class="">THE CHANGING REPRESENTATION OF ABORIGINAL AND TORRES STRAIT ISLANDER PEOPLES IN FILM</span></span><br />\n            <span class="subheading2">FRIDAY 31 MAY, 6.30PM</span><br />\n            <br />\n            <span class="normaltext1"> Join the National Indigenous Television Network, legend </span><span class="normaltext1"><span class="">Uncle Noel Tovey, producer/director John Harvey, feminist blogger Celeste Liddle</span><span class=""> </span><span class="">and Margaret Harvey</span><span class=""> in a dynamic discussion about how Aboriginal and Torres Strait Islander peoples are claiming their place in the Australian film industry. </span><span class="">Moderated by Todd Condie, Indigenous Policy Officer, City of Port Phillip, </span><span class="">this forum will be held on the land of the Yalukit Wilum clan of the Boon Wurrung language group of the greater Kulin Nation.</span></span><br />\n            <br />\n            </p>\n            <p><span class="normaltext1">John Harvey is an emerging writer, director and producer in film and theatre with his company Brown Cab Productions and is currently working with highly acclaimed filmmaker Robert Connolly as a Producer Intern. Enduring a poverty-stricken and turbulent start to life, Noel Tovey (pictured) has led an esteemed career spanning 60 years as an actor, director, dancer, singer, choreographer, designer, w</span><span class="normaltext1"><span class="">riter and teacher – a career that has spanned Europe and Australia. Celeste Liddle is an Arrernte woman, National Indigenous Organiser of the NTEU and feminist blogger. She completed her BA Honours in theatre and drama in 2002 looking at identity politics in Indigenous Australian theatre.</span> Margaret Harvey is an emerging stage and film director based in Melbourne.  Her passion lies in exploring cross art forms to tell a story. <br />\n            <br />\n            </span></p>\n            <p class="normaltext1">Presented with the assistance of the National Film and Sound Archive of Australia <br />\n            </p>\n            <p><br />\n            <img alt="" src="/skf2015/uploads/NFSA-Mono-REV.png" width="153" height="88" /><br />\n            <br />\n            </p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FD-NoelTovey.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />', '2013-04-12 15:17:22', '2013-05-28 15:30:04'),
(200, 7, 'easy-come-easy-go', 'Easy Come Easy Go', '<p><span class="heading"><img alt="" src="/skf2015/uploads/Easybeats-ResizedforWeb-2.jpg" width="710" height="300" /></span></p>\n<span class="heading"><br />\nEASY COME, EASY GO\n</span><br />\n<span class="subheading1">A SALUTE TO AUSSIE POP IN THE SIXTIES</span><br />\n<span class="subheading2"><br />\nMONDAY 27 MAY, 7.30PM<br />\nTHE ASTOR THEATRE</span><br />\n<br />\n<a target="_blank" href="http://www.ticketmaster.com.au/event/13004A8FE0C2A89F"><img alt="" src="/skf2015/uploads/BUY-TICKETS-BUTTON.jpg" width="124" height="30" /></a><br />\n<p><span class="normaltext1"><br />\n<br />\nA night of quintessential sixties Aussie pop starring The Easybeats in the long lost documentary <em>Easy Come, Easy Go</em>, not screened in Melbourne in over 40 years</span><span class="normaltext1">.<br />\n<br />\nThis session proudly supported by the National Film and Sound Archive of Australia</span><br />\n<br />\n<br />\n<a target="_blank" href="http://www.nfsa.gov.au/"><img alt="" src="/skf2015/uploads/NFSA-Mono-REV.png" width="165" height="96" /></a><br />\n</p>\n<span class="normaltext1">\n</span>\n<hr />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <span class="subheading1">EASY COME, EASY GO</span><br />\n            <span class="normaltext1">(Director: Peter Clifton, 1967/2011, 33.08mins)<br />\n            <br />\n            Director Peter Clifton shot his Easybeats documentary in 1967, but the negative and the print vanished during post-production and the project was forgotten…until recently. Clifton has restored the film to a 35-minute version which will have its Victorian premiere screening at the St Kilda Film Festival.<br />\n            <br />\n            See the band perform their international hit ‘Friday On My Mind’, flee teenyboppers down Carnaby Street, visit the pirate broadcaster Radio Caroline and enjoy a footy match with the Small Faces and much more! If you’re a fan of Vanda and Young or Little Stevie Wright, this is surely must-see viewing, with a soundtrack of hits and rarities pounding out through the Astor sound system.<br />\n            <br />\n            The new version has been restored with the assistance of the National Film and Sound Archive. Director Peter Clifton will introduce the screening.</span><br />\n            <p><font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/Easybeats-Small-Right.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <span class="subheading1">MELDRUM 1971</span><br />\n            <span class="normaltext1">(Director: Peter Lamb, 1971, 8mins)<br />\n            </span><span class="normaltext1"><span class=""><br />\n            </span>In November 1974 Australia''s most popular music show, Countdown, premiered on ABC television. A few months later pop guru Ian " Molly " Meldrum became the face of the show with his shambolic interviews, enthusiastic endorsements of numerous bands and general gossip. Molly was already an industry veteran with his ties to Go-Set magazine as a pop columnist and as a producer, most notably on Russell Morris'' ‘The Real Thing’ (1969) plus hits for Ronnie Burns and Zoot.<br />\n            <br />\n            But Countdown was not Molly''s first stab at television. In the late 1960''s he was a regular on Kommotion and Uptight, but he had yet to host his own show. In 1971 an opportunity presented itself when he starred in an eight-minute pilot in which he pitched '' a new concept in television '' - a music-pop culture show aimed squarely at teen and young adult tastes. The pitch, directed by Garry Wapshott and Peter Lamb, is a fascinating time capsule. Molly is seen in full sartorial splendour (minus his later trademark hat) speaking direct to camera, passionately and (would you believe) most eloquently. This footage has never been screened in public.<br />\n            <br />\n            "We reject false slickness and intend to present our program with honesty and integrity, creative value and commercial appeal,” Molly Meldrum enthuses.<br />\n            <br />\n            The pitch fell on deaf ears at the time, but three years later a legend was born.<br />\n            <br />\n            Meldrum is a late addition to the Easy Come Easy Go program. Also added to the line-up are rare clips from Axiom (‘Arkansas Grass’) and The Twilights (‘Cathy Come Home’, ‘The Way They Played’).</span><br />\n            <p><font face="Times New Roman"><font face="Times New Roman"><span class="normaltext3"><font face="Times New Roman"><font class="normaltext3" face="Times New Roman"></font></font></span> </font></font></p>\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/Meldrum-SmallRightPic.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;"><span class="subheading1">APPROXIMATELY PANTHER</span><br />\n            <span class="normaltext1">(Dir: Peter Lamb, 1966, 22.26mins)<br />\n            <br />\n            <em>Approximately Panther</em> is a unique document of the burgeoning Melbourne music scene as it existed nearly half a century ago. Shot by Peter Lamb and Garry Wapshott, the film features rare footage of bands and venues; a fascinating time capsule into a vanished era of once-groovy fashion and trendy discotheques.<br />\n            <br />\n            Follow Doug Panther, ace hipster and go-set journalist through Swingin'' Melbourne circa 1967. The cast of characters includes singer Lynne Randell, model Jennie Ham, the skivvie-clad 3AK Good Guy disc jockey Lionel Yorke and pop sensation Normie Rowe, mobbed by adoring fans at Essendon Airport. We also catch a glimpse of Melbourne''s favourite cult group The Loved Ones.</span></td>\n            <td style="padding-top: 15px;" valign="top" width="10"><br />\n            </td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/Approximately-Sm-R.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" class="normaltext1" valign="top"><span class="subheading1">FUN RADIO</span><br />\n            <span class="normaltext1">(Dir: Nigel Buesst, 1964, 9.22mins)<br />\n            <br />\n            Don''t let anyone tell you that Melbourne didn''t rock in the sixties. The epicentre was The Greater 3UZ (930AM), the radio station with its finger on the pulse of teen tastes. Director Nigel Buesst, a founding father of St Kilda Film Festival, focuses his springwind Bolex camera on Professor Fun (Don Lunn), the city''s most popular disc jockey of the time, presiding over a kaleidoscope of jingles, news flashes, promos, hit singles and lame jokes.</span></td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="/skf2015/uploads/FunRadio-Sm-R.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />\n<table style="margin-top: 20px;">\n    <tbody>\n        <tr>\n            <td style="vertical-align: top;" valign="top">\n            <p class="subheading1">ALSO SCREENING</p>\n            <p><span class="subheading2"><br />\n            SAD DARK EYES</span> <br />\n            </p>\n            <span class="normaltext1">(Dir: Peter Lamb, 1967, 2.14mins)<br />\n            <br />\n            A rarely seen music video by The Loved Ones. &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp;</span> <br />\n            </td>\n            <td style="padding-top: 15px;" valign="top" width="10">&nbsp;</td>\n            <td valign="top" width="230"><img alt="" src="http://www.stkildafilmfestival.com.au/skf2015/uploads/TheLovedOnes-SmallRight.jpg" width="230" height="155" /></td>\n        </tr>\n    </tbody>\n</table>\n<hr style="margin-top:20px!important;" />', '2013-04-15 15:54:25', '2013-05-14 17:04:16'),
