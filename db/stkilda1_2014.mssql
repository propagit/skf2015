(97, 9, 'festival-club', 'FESTIVAL CLUB', '<p><span class="heading"><span class="normaltext1"><span class="normaltext1"><img style="width: 320px; height: 146px" border="0" alt="" src="/skf2015/uploads/Renault-Festival-Lounge.jpg" width="320" height="146" /></span></span>\n<hr />\n</span>\n<p>\n<table style="padding-top: 10px">\n    <tbody>\n        <tr>\n            <td class="heading"></td>\n            <td style="padding-top: 15px" valign="top" rowspan="5" width="250"><span class="normaltext1">&nbsp;</span></td>\n            <td style="padding-top: 15px" valign="top" rowspan="5" width="250"><br />\n            <span class="normaltext1"><img border="0" alt="" src="/skf2015/uploads/Festival Lounge.jpg" width="250" height="201" /></span></td>\n        </tr>\n        <tr>\n            <td style="width: 980px; height: 215px" class="normaltext1">\n            <p>UPSTAIRS AT THE ASTOR THEATRE&nbsp;</p>\n            <br />\n            <p><span class="normaltext1"><span class="normaltext1">The Renault Festival Lounge is upstairs at The Astor Theatre.<br />\n            <br />\n            ItÃ¢â‚¬â„¢s the perfect place to relax, talk film with friends, network and soak up the atmosphere.<br />\n            <br />\n            Have a coffee, indulge in a cocktail, or while away the night with a glass of wine, cider or beer.<br />\n            <br />\n            For a week in May Ã¢â‚¬â€œ itÃ¢â‚¬â„¢s the best club in town.</span></span></p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n        <tr>\n            <td>\n            <p><span class="normaltext1"><em></em></span>&nbsp;</p>\n            </td>\n            <td>&nbsp;</td>\n        </tr>\n    </tbody>\n</table>\n</p>\n', '2011-05-02 12:16:48', '2012-04-25 17:00:16');
